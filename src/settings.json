{
    "source": "rtsp://admin:password@192.168.1.100:554/ch01/0",
    "storage_path": "c:\\tmp",
    "errors_before_reconnect": 5,
    "delay_after_error_ms": 2000,
    "cooldown_write_time_ms": 5000,
    "buffer_overflow_strategy": "delay",

    "detection_engine": "CodeprojectAI",
    "codeproject_ai_url": "http://localhost:32168/v1/vision/custom/ipcam-general",
    "onnx_file_path": "yolov5s.onnx",
    "min_confidence": 0.4,
    "motion_detect_settings" : {
        "gaussian_blur_sz" : 20,
        "threshold" : 15,
        "area_trigger" : 150
    },
    "nth_detect_frame": 5,
    "use_image_scale": true,
    "img_scale_x": 0.5,
    "img_scale_y": 0.5,
    "img_format": "jpg",
    "frame_color": {
        "R": 80,
        "G": 245,
        "B": 95
    },
    "frame_width_px": 1,
    "use_video_scale": false,
    "video_width": 1024,
    "video_height": 576,

    "bot_token": "TOKEN",
    "allowed_users": [
        900000000,
        900000001
    ],
    "alarm_notification_delay_ms": 30000,
    "preview_sampling_interval_ms": 2000,
    "send_video_previews": true,
    "send_video": false,

    "log_level": "info",
    "log_filename": "",
    "notify_on_start": true
}
